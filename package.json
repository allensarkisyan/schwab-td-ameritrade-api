{
  "name": "@allensarkisyan/schwab-td-ameritrade-api",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "author": "Allen Sarkisyan",
  "main": "./dist/cjs/td-api.js",
  "module": "./dist/esm/td-api.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/td-api.js"
      },
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/td-api.js"
      },
      "default": "./dist/esm/td-api.js"
    },
    "./td-utils": {
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/td-utils.js"
      },
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/td-utils.js"
      },
      "default": "./dist/esm/td-utils.js"
    }
  },
  "scripts": {
    "build:types": "tsc --declaration --project tsconfig.types.json",
    "build:esm": "tsc --project tsconfig.json --module esnext --outDir ./dist/esm",
    "build:cjs": "tsc --project tsconfig.json --module commonjs --outDir ./dist/cjs",
    "build": "npm run build:types && npm run build:esm && npm run build:cjs && cd dist && prettier . --write",
    "test": "jest --verbose"
  },
  "dependencies": {
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@types/node": "20.6.0",
    "jest": "29.7.0",
    "prettier": "3.0.3",
    "typescript": "5.2.2"
  },
  "prettier": {
    "semi": true,
    "trailingComma": "all",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "arrowParens": "always"
  }
}
