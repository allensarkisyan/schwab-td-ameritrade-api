{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "docs-readme": "echo \"---\nslug: /\n---\n# README\n\n$(cat ../README.md)\" > docs/README.md",
    "docs-api": "npx jsdoc2md {../dist/esm/td-api.js,../dist/esm/td-utils.js,../dist/esm/@types/jsdoc.js} > docs/td-api.md",
    "docs": "npm run docs-readme && npm run docs-api && npm run build && npm run serve",
    "jsdoc": "npx jsdoc@latest -r ../dist/esm"
  },
  "dependencies": {
    "@docusaurus/core": "2.4.1",
    "@docusaurus/preset-classic": "2.4.1",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.2.1",
    "lucide-react": "^0.276.0",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",

    "jsdoc-to-markdown": "8.0.0",
    "mermaid": "10.4.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.4.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  }
}
